/* pages/project1/project1.wxss */

.project1 {
  display: flex;
  flex-direction: column;
  font-size: 30rpx;
  background: #dec26f;
  padding: 30rpx 30rpx 10rpx;
}

text {
  display: block;
}

.logo {
  margin: 0 auto 10rpx;
}

.logo image {
  width: 120rpx;
  height: 120rpx;
}

.introduce {
  text-align: center;
  padding-bottom: 40rpx;
}

.content view:nth-of-type(1) {
  font-size: 40rpx;
}

.content view {
  margin-bottom: 20rpx;
}

.content .banner {
  margin-bottom: 0;
}

.content {
  margin-bottom: 40rpx;
}
.content:last-of-type {
  margin-bottom: 0;
}

.banner {
  display: flex;
  justify-content: space-between;
}

.banner image {
  width: 300rpx;
  height: 600rpx;
}
