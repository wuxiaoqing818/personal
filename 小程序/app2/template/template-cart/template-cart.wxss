/* pages/cart/cart.wxss */

.cart {
  display: flex;
  flex-direction: column;
  font-size: 26rpx;
}

.goods {
  width: 100%;
  height: 100%;
  /* overflow-y: scroll; */
  white-space: nowrap;
  padding-left: 20rpx;
  padding-bottom: 120rpx;
}

.goodsContent {
  width: 100%;
  display: flex;
  border-bottom: 2rpx solid #eee;
}

.check {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  border: 2rpx solid #ccc;
  margin-right: 20rpx;
}

.goodsContent .active {
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  border: 2rpx solid #d81e06;
}

.goodsTop>image {
  width: 160rpx;
  height: 160rpx;
}

.goodsBot {
  position: relative;
  width: 60%;
  padding-left: 20rpx;
}

.goodsBot>text {
  display: inline-block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #ccc;
  padding-bottom: 20rpx;
}

.goodsBot>text:nth-of-type(1) {
  display: block;
  color: black;
  padding-bottom: 20rpx;
}

.goodsBot>text:last-of-type {
  display: block;
  color: red;
  padding-bottom: 0;
}

.goodsBot>view {
  position: absolute;
  right: 0;
  bottom: 0;
  font-size: 24rpx;
  display: flex;
}

.goodsBot>view>input {
  width: 60rpx;
  text-align: center;
  height: 40rpx;
  min-height: 0;
  border-top: 2rpx solid #ccc;
  border-bottom: 2rpx solid #ccc;
}

.goodsBot>view>text {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  border: 2rpx solid #ccc;
  text-align: center;
  line-height: 40rpx;
}

.footer {
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  display: flex;
  box-shadow: 0 0 30rpx 0 rgba(0, 0, 0, 0.5);
  background: white;
  z-index: 1000;
}

.foTop {
  width: 70%;
  display: flex;
  align-items: center;
  padding-left: 20rpx;
  color: orangered;
}

.foBot {
  width: 30%;
}

.foBot button {
  margin: 0;
  padding: 0;
  min-height: 0;
  width: 100%;
  height: 100%;
  border-radius: 0;
  color: white;
  display: flex;
  align-items: center;
  justify-content: space-around;
  background: orangered;
}

.foBot button:not([size='mini']) {
  width: 100%;
  height: 100%;
  border: none;
  margin: 0;
  padding: 0;
}

.remove {
  background-color: orangered;
  width: 90px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  -webkit-transform: translateX(90px);
  transform: translateX(90px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}

.touch-move-active .item, .touch-move-active .remove {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

.item {
  padding: 20rpx 0;
  display: flex;
  align-items: center;
  background-color: #fff;
  position: relative;
  z-index: 99;
  width: 100%;
  margin-right: 0;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(90px);
  transform: translateX(90px);
  margin-left: -90px;
}
