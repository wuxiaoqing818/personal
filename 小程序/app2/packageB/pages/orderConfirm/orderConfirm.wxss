/* pages/orderConfirm/orderConfirm.wxss */

.orderConfirm {
  display: flex;
  flex-direction: column;
  background: #eee;
  font-size: 28rpx;
}

.goods {
  padding-left: 30rpx;
  background: white;
  margin-bottom: 20rpx;
}

.goods>view:nth-of-type(1) {
  margin-bottom: 30rpx;
}

.goodsDetail {
  display: flex;
  justify-content: space-between;
  padding: 30rpx 30rpx 30rpx 0;
  border-top: 2rpx solid #ccc;
}

.goodsDetail image {
  width: 180rpx;
  height: 180rpx;
  margin-right: 10rpx;
}

.goodsDetail>view {
  width: 70%;
}

.goodsDetail>view>view {
  display: flex;
  justify-content: space-between;
  color: #ccc;
}

.goodsDetail>view>view:nth-of-type(1) {
  margin-bottom: 100rpx;
  color: black;
}

.goodsDetail>view>view text {
  display: inline-block;
}

.goodsDetail>view>view:nth-of-type(1) text:nth-of-type(1) {
  width: 340rpx;
}

.send {
  background: white;
  margin-bottom: 100rpx;
}

.sendTop {
  display: flex;
  padding: 20rpx 30rpx;
  border-bottom: 2rpx solid #ccc;
  align-items: center;
}

.sendTop>text {
  width: 50%;
}

.sendTop>view {
  font-size: 32rpx;
  display: flex;
  align-items: center;
}

.sendTop>view>view {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  border: 2rpx solid #ccc;
  margin-right: 10rpx;
}

.sendTop>view:nth-of-type(1) {
  margin-right: 10rpx;
}

.sendTop .active {
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  border: 2rpx solid #1afa29;
}

.sendMid {
  padding: 40rpx 30rpx;
  border-bottom: 2rpx solid #ccc;
}

.sendMid text {
  display: block;
}

.sendMid text:last-of-type {
  color: #ccc;
  margin-top: 20rpx;
}

.sendCheck {
  padding: 40rpx 30rpx;
  border-bottom: 2rpx solid #ccc;
  display: flex;
  align-items: center;
}

.sendCheck image {
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
}

.sendBot {
  display: flex;
  padding: 30rpx 40rpx 20rpx 30rpx;
  justify-content: space-between;
  align-items: center;
}

.sendBot input {
  width: 80%;
}

.footer {
  position: fixed;
  bottom: 0;
  display: flex;
  width: 100%;
  height: 100rpx;
  background: white;
  color: orangered;
}

.footer>view:nth-of-type(1) {
  width: 70%;
  height: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.footer>view:nth-of-type(2) {
  width: 30%;
  height: 100%;
  color: white;
  background: orangered;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
